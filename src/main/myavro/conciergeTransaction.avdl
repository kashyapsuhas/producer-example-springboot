@namespace("com.bazaarvoice.legion.schema.emodb.transaction.consumer")
protocol TransactionConsumerProtocol {

	record TransactionConsumer {

		union {null, string} customername = null;
		union {null, int}  amount = null;
		union {null, boolean} status = null;

	}

}